{
	"version": "0.2",
	"language": "en",
	"caseSensitive": false,
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"words": [],
	"useGitignore": true,
	"ignorePaths": [
		".cicleci/*",
		".github/*",
		".git/*",
		".git/!{COMMIT_EDITMSG,EDITMSG}",
		".git/*/**",
		".yarn",
		".pnp.js",
		"**/.git/**",
		".vscode",
		".gitignore",
		"action/lib/**",
		"coverage",
		".cspell.json",
		"cspell.json",
		".gitbook.yaml",
