		"**/node_modules/**",
		"**/vscode-extension/**",
		"/**/package-lock.json",
		"/**/yarn.lock",
		"**/assets/i18n/*.json",
		"**/*.svg",
		"docker-compose.demo.yml",
		"docker-compose.yml",
		"/**/*.{svg,css,scss}",
